{
    "kind": "collectionType",
    "collectionName": "showcases",
    "info": {
        "singularName": "showcase",
        "pluralName": "showcases",
        "displayName": "showcase",
        "description": ""
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "summary": {
            "type": "text",
            "maxLength": 155,
            "required": true
        },
        "slug": {
            "type": "uid",
            "targetField": "name"
        },
        "client": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "api::client.client"
        },
        "name": {
            "type": "string",
            "required": true
        },
        "services": {
            "type": "relation",
            "relation": "manyToMany",
            "target": "api::service.service",
            "inversedBy": "showcases"
        },
        "event": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "api::event.event"
        },
        "seo": {
            "type": "component",
            "repeatable": false,
            "component": "shared.seo",
            "required": true
        },
        "featured": {
            "type": "boolean"
        },
        "leftSections": {
            "type": "dynamiczone",
            "components": [
                "sections.contact-form",
                "sections.cyclingsentence",
                "sections.event-partners",
                "sections.full-size-carousel",
                "sections.image-title",
                "sections.large-summary",
                "sections.services-showcase",
                "sections.showcases",
                "sections.simple-content",
                "sections.video-hero",
                "sections.testimonials"
            ],
            "required": true
        },
        "imageTitle": {
            "type": "component",
            "repeatable": false,
            "component": "sections.image-title",
            "required": true
        },
        "bottomSections": {
            "type": "dynamiczone",
            "components": [
                "sections.contact-form",
                "sections.cyclingsentence",
                "sections.event-partners",
                "sections.full-size-carousel",
                "sections.image-title",
                "sections.large-summary",
                "sections.services-showcase",
                "sections.showcases",
                "sections.simple-content",
                "sections.video-hero",
                "sections.testimonials"
            ],
            "required": false
        }
    }
}
