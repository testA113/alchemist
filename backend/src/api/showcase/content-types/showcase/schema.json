{
  "kind": "collectionType",
  "collectionName": "showcases",
  "info": {
    "singularName": "showcase",
    "pluralName": "showcases",
    "displayName": "showcase",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "maxLength": 60,
      "required": true
    },
    "summary": {
      "type": "text",
      "maxLength": 155,
      "required": true
    },
    "hero": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "client": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::client.client"
    },
    "name": {
      "type": "string",
      "required": true
    },
    "services": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::service.service",
      "inversedBy": "showcases"
    },
    "event": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::event.event"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    }
  }
}
